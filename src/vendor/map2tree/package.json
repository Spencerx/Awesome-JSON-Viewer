  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/lodash-es": "^4.17.12",
    "immutable": "^4.3.7",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.5.4"
